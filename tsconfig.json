{
	"compilerOptions": {
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		"moduleResolution": "bundler",
		"outDir": "dist",
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"module": "preserve",
		"lib": ["es2022", "dom", "dom.iterable"]
	}
}
